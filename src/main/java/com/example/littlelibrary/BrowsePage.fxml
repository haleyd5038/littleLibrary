            <TableColumn fx:id="titleColumn" prefWidth="133.0" text="Title" />
            <TableColumn fx:id="authorColumn" minWidth="3.0" prefWidth="109.0" text="Author" />
            <TableColumn fx:id="finishDateColumn" minWidth="0.0" prefWidth="83.0" text="Finish Date" />
            <TableColumn fx:id="pageCountColumn" prefWidth="78.0" text="Page Count" />
            <TableColumn fx:id="ratingColumn" prefWidth="60.0" text="Rating" />
            <TableColumn fx:id="idColumn" prefWidth="48.0" text="id" />
         </columns>
      </TableView>
      <Button layoutX="428.0" layoutY="352.0" mnemonicParsing="false" onAction="#deleteEntry" text="Delete" />
      <Button layoutX="489.0" layoutY="352.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="51.0" text="Edit" />
